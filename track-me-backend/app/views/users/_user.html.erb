<div id="<%= dom_id user %>">
  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Verified:</strong>
    <%= user.verified %>
  </p>

  <p>
    <strong>Imageurl:</strong>
    <%= user.imageUrl %>
  </p>

</div>
